    "version": "0.2.0",
    "configurations": [


        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:7357",
            "webRoot": "${workspaceFolder}",
          //  "runtimeArgs": ["--disable-web-security"],
        },

        {
            "name": "av-app",
            "request": "launch",
            "type": "dart",
            //"args": ["--web-port", "7357"]
        },
        {
            "name": "av-app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "av-app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}